<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lily.lilytest">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".metrial_design.Metrial_Design_Activity" />
        <activity
            android:name=".metrial_design.toolbar.ToolBarActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".metrial_design.appbarlayout.AppBarActivity" />
        <activity android:name=".metrial_design.appbarlayout.CollapsingToolBarActivity" />
        <activity android:name=".Retrofit.TestRetroft" />
        <activity android:name=".metrial_design.toolbar.SideMenuActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".home.BottomNavigationActivity"/>


    </application>

</manifest>